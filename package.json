{
    "name": "pz-wallet",
    "displayName": "PZ Wallet",
    "description": "A third-party VSCode extension for Lua templates for \"Project Zomboid\".",
    "version": "41.78.0",
    "engines": {
        "vscode": "^1.75.0"
    },
    "icon": "./icon.png",
    "publisher": "asledgehammer",
    "author": {
        "name": "asledgehammer"
    },
    "activationEvents": [
        "*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/asledgehammer/PZ-Wallet"
    },
    "categories": [
        "Snippets"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/luaparse": "^0.2.9",
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.75.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "typescript": "^5.0.2"
    },
    "license": "MIT",
    "dependencies": {
        "luaparse": "^0.3.1",
        "simple-git": "^3.19.1"
    },
    "contributes": {
        "commands": [
            {
                "command": "wallet-reload-templates",
                "title": "PZ-Wallet: Reload Templates"
            }
        ]
    }
}